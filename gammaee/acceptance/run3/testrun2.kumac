sigma pa  = $HINFO(21,'ENTRIES') - ptobge*($HINFO(41,'ENTRIES') - bgecos*$HINFO(61,'ENTRIES')) - ptobgp*($HINFO(51,'ENTRIES') - bgpcos*$HINFO(61,'ENTRIES')) - ptocos*$HINFO(61,'ENTRIES')
sigma pb  = $HINFO(22,'ENTRIES') - ptobge*($HINFO(42,'ENTRIES') - bgecos*$HINFO(62,'ENTRIES')) - ptobgp*($HINFO(52,'ENTRIES') - bgpcos*$HINFO(62,'ENTRIES')) - ptocos*$HINFO(62,'ENTRIES')
sigma ca  = $HINFO(31,'ENTRIES') - ctobge*($HINFO(41,'ENTRIES') - bgecos*$HINFO(61,'ENTRIES')) - ctobgp*($HINFO(51,'ENTRIES') - bgpcos*$HINFO(61,'ENTRIES')) - ctocos*$HINFO(61,'ENTRIES')
sigma cb  = $HINFO(32,'ENTRIES') - ctobge*($HINFO(42,'ENTRIES') - bgecos*$HINFO(62,'ENTRIES')) - ctobgp*($HINFO(52,'ENTRIES') - bgpcos*$HINFO(62,'ENTRIES')) - ctocos*$HINFO(62,'ENTRIES')
sigma ma  = $HINFO(71,'ENTRIES')
sigma mb  = $HINFO(72,'ENTRIES')
sigma m2a = $HINFO(81,'ENTRIES')
sigma ptoc   = (pa*1.0*mb/ma - pb)/(ca*1.0*mb/ma - cb)
sigma ptocup = (pa*1.2*mb/ma - pb)/(ca*1.2*mb/ma - cb)
sigma ptocdo = (pa*0.8*mb/ma - pb)/(ca*0.8*mb/ma - cb)
sigma ptomc  = (pa - pb*ca/cb)/(ma - mb*ca/cb)
sigma ptomc2 = (pa - pb*ca/cb)/(1. - (ca/cb)*(mb/ma))/m2a

message '${\tt pa}  =' $HINFO(21,'ENTRIES') '-' $SIGMA(ptobge) '* (' $HINFO(41,'ENTRIES') '-' $SIGMA(bgecos) '*' $HINFO(61,'ENTRIES') ') -' $SIGMA(ptobgp) '* (' $HINFO(51,'ENTRIES') '-' $SIGMA(bgpcos) '*' $HINFO(61,'ENTRIES') ') -' $SIGMA(ptocos) '*' $HINFO(61,'ENTRIES') '=' $SIGMA(pa) '$ \\'
message '${\tt pb}  =' $HINFO(22,'ENTRIES') '-' $SIGMA(ptobge) '* (' $HINFO(42,'ENTRIES') '-' $SIGMA(bgecos) '*' $HINFO(62,'ENTRIES') ') -' $SIGMA(ptobgp) '* (' $HINFO(52,'ENTRIES') '-' $SIGMA(bgpcos) '*' $HINFO(62,'ENTRIES') ') -' $SIGMA(ptocos) '*' $HINFO(62,'ENTRIES') '=' $SIGMA(pb) '$ \\'
message '${\tt ca}  =' $HINFO(31,'ENTRIES') '-' $SIGMA(ctobge) '* (' $HINFO(41,'ENTRIES') '-' $SIGMA(bgecos) '*' $HINFO(61,'ENTRIES') ') -' $SIGMA(ctobgp) '* (' $HINFO(51,'ENTRIES') '-' $SIGMA(bgpcos) '*' $HINFO(61,'ENTRIES') ') -' $SIGMA(ctocos) '*' $HINFO(61,'ENTRIES') '=' $SIGMA(ca) '$ \\'
message '${\tt cb}  =' $HINFO(32,'ENTRIES') '-' $SIGMA(ctobge) '* (' $HINFO(42,'ENTRIES') '-' $SIGMA(bgecos) '*' $HINFO(62,'ENTRIES') ') -' $SIGMA(ctobgp) '* (' $HINFO(52,'ENTRIES') '-' $SIGMA(bgpcos) '*' $HINFO(62,'ENTRIES') ') -' $SIGMA(ctocos) '*' $HINFO(62,'ENTRIES') '=' $SIGMA(cb) '$ \\'
message '${\tt ma}  =' $HINFO(71,'ENTRIES') '$ \\'
message '${\tt mb}  =' $HINFO(72,'ENTRIES') '$ \\'
message '${\tt m2a} =' $HINFO(81,'ENTRIES') '$ \\'
message '${\tt ptoc\mbox{ }\mbox{ }}   = (' $SIGMA(pa) '* 1.0 *' $SIGMA(mb) '/' $SIGMA(ma) '-' $SIGMA(pb) ') / (' $SIGMA(ca) '* 1.0 *' $SIGMA(mb) '/' $SIGMA(ma) '-' $SIGMA(cb) ')' '=' $SIGMA(ptoc) '$ \\'
message '${\tt ptocup} = (' $SIGMA(pa) '* 1.2 *' $SIGMA(mb) '/' $SIGMA(ma) '-' $SIGMA(pb) ') / (' $SIGMA(ca) '* 1.2 *' $SIGMA(mb) '/' $SIGMA(ma) '-' $SIGMA(cb) ')' '=' $SIGMA(ptocup) '$ \\'
message '${\tt ptocdo} = (' $SIGMA(pa) '* 0.8 *' $SIGMA(mb) '/' $SIGMA(ma) '-' $SIGMA(pb) ') / (' $SIGMA(ca) '* 0.8 *' $SIGMA(mb) '/' $SIGMA(ma) '-' $SIGMA(cb) ')' '=' $SIGMA(ptocdo) '$ \\'
message '${\tt ptomc}  = (' $SIGMA(pa) '-' $SIGMA(pb) '*' $SIGMA(ca) '/' $SIGMA(cb) ') / (' $SIGMA(ma) '-' $SIGMA(mb) '*' $SIGMA(ca) '/' $SIGMA(cb) ')' '=' $SIGMA(ptomc) '$ \\'
message '${\tt ptomc2} = (' $SIGMA(pa) '-' $SIGMA(pb) '*' $SIGMA(ca) '/' $SIGMA(cb) ') / ( 1. - (' $SIGMA(ca) '/' $SIGMA(cb) ') * (' $SIGMA(mb) '/' $SIGMA(ma) ') ) /' $SIGMA(m2a) '=' $SIGMA(ptomc2) '$ \\'

