export USER_GOODEVENTS=`fgrep ' %% SYSTEM-Processor.CountEvents: goodEvents' /cdat/tem/mccann/counting_logs/tmp130508.log | awk '{print $4}'`
export USER_COSMICEVENTS=`fgrep ' %% SYSTEM-Processor.CountEvents: goodEvents' /cdat/tem/mccann/counting_logs/tmp130508.log | awk '{print $6}'`
export USER_BGPEVENTS=`fgrep ' %% SYSTEM-Processor.CountEvents: goodEvents' /cdat/tem/mccann/counting_logs/tmp130508.log | awk '{print $8}'`
export USER_BGEEVENTS=`fgrep ' %% SYSTEM-Processor.CountEvents: goodEvents' /cdat/tem/mccann/counting_logs/tmp130508.log | awk '{print $10}'`
rm -f /cdat/tem/mccann/counting_logs/tmp130508.log

echo '$goodEvents{130508} =' $USER_GOODEVENTS >> goodEvents.pl
echo '$cosmicEvents{130508} =' $USER_COSMICEVENTS >> cosmicEvents.pl
echo '$bgpEvents{130508} =' $USER_BGPEVENTS >> bgpEvents.pl
echo '$bgeEvents{130508} =' $USER_BGEEVENTS >> bgeEvents.pl

exit
