cd /home/mccann/fits/
. /nfs/cleo3/scripts/cleo3defs
c3rel Dec10_03

export USER_DB=27
export USER_RUN=130508

echo $USER_DB $USER_RUN

suez -f count.tcl | tee /cdat/tem/mccann/counting_logs/tmp130508.log
